<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="studymeteam.studymea.HomeTab"
    android:background="#44940e">

    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignStart="@+id/textView8"
            android:layout_marginTop="22dp"
            android:text="Welcome Home!"
            android:textAlignment="center"
            android:textColor="#000d5d"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView6"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="20dp"
            android:hint="Name" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView7"
            android:layout_marginTop="21dp"
            android:layout_toStartOf="@+id/textView14"
            android:text="Age" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/textView8"
            android:layout_below="@+id/textView8"
            android:text="Level" />

        <TextView
            android:id="@+id/userAge"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView8"
            android:layout_alignBottom="@+id/textView8"
            android:layout_alignStart="@+id/userSnacks"
            android:hint="User Age" />

        <TextView
            android:id="@+id/userLevel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/userAge"
            android:layout_alignTop="@+id/textView9"
            android:hint="User Level" />

        <TextView
            android:id="@+id/userSnacks"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView7"
            android:layout_alignBottom="@+id/textView7"
            android:layout_toEndOf="@+id/textView14"
            android:hint="Snack No." />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/userLevel"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="64dp"
            android:text="Progress"
            android:textColor="#000d5d"
            android:textStyle="bold"
            android:visibility="visible" />

        <ProgressBar
            android:id="@+id/progressBar"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/userLevel"
            android:layout_alignStart="@+id/userLevel"
            android:layout_below="@+id/textView14"
            android:layout_marginTop="18dp"
            android:max="100"
            android:progress="50" />

        <ProgressBar
            android:id="@+id/progressBar2"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/progressBar3"
            android:layout_alignEnd="@+id/progressBar"
            android:layout_alignStart="@+id/progressBar"
            android:max="100"
            android:progress="30" />

        <ProgressBar
            android:id="@+id/progressBar3"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView18"
            android:layout_alignEnd="@+id/progressBar2"
            android:layout_alignStart="@+id/progressBar2"
            android:max="100"
            android:progress="10" />

        <ProgressBar
            android:id="@+id/progressBar4"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/progressBar3"
            android:layout_alignStart="@+id/progressBar3"
            android:layout_below="@+id/progressBar3"
            android:layout_marginTop="30dp"
            android:max="100"
            android:progress="60" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/textView9"
            android:layout_alignTop="@+id/progressBar"
            android:text="Math Lectures" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/textView15"
            android:layout_below="@+id/textView15"
            android:text="Math Weekly Tests" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/textView17"
            android:layout_below="@+id/textView17"
            android:text="Math Monthly Tests" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/progressBar4"
            android:layout_alignEnd="@+id/textView18"
            android:text="Biology Lectures" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/textView19"
            android:layout_below="@+id/textView19"
            android:text="Biology Weekly Tests" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/textView20"
            android:layout_below="@+id/textView20"
            android:text="Biology Monthly Tests" />

        <ProgressBar
            android:id="@+id/progressBar5"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/progressBar4"
            android:layout_alignStart="@+id/progressBar4"
            android:layout_alignTop="@+id/textView20"
            android:max="100"
            android:progress="30" />

        <ProgressBar
            android:id="@+id/progressBar6"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView21"
            android:layout_alignEnd="@+id/progressBar5"
            android:layout_alignStart="@+id/progressBar5"
            android:max="100"
            android:progress="10" />

    </RelativeLayout>

</FrameLayout>
